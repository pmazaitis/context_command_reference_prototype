                    <stentry>0.25\bodyfontlineheight</stentry>
                </strow>
                <strow>
                    <stentry>formula</stentry>
                    <stentry>\medskipamount</stentry>
                    <stentry>0.5\bodyfontlineheight</stentry>
                </strow>
                <strow>
                    <stentry>white</stentry>
